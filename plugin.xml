<?xml version="1.0" encoding="UTF-8"?>

<plugin>
   <class>org.igniterealtime.openfire.s2sconformancetest.S2STestPlugin</class>
   <name>${project.name}</name>
   <description>${project.description}</description>
   <author>Dan Caseley</author>
   <version>${project.version}</version>
   <date>2023-11-28</date>
   <minServerVersion>4.8.0-beta</minServerVersion>
   <minJavaVersion>11</minJavaVersion>
   <adminconsole>
       <tab id="tab-server">
           <sidebar id="sidebar-server-settings">
               <item id="tests"
                     url="tests.jsp"
                     name="S2S Test"
                     description="Click to run S2S connection tests"
               />
           </sidebar>
       </tab>
    </adminconsole>

    <csrfProtectionEnabled>true</csrfProtectionEnabled>
</plugin>